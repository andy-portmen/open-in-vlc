{
   "author": "Andy Portmen",
   "background": {
      "scripts": [ "common.js" ]
   },
   "description": "Adds a context menu to send audio and video streams directly to the well-known VLC™ media player",
   "homepage_url": "https://add0n.com/open-in-vlc.html",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "19": "data/icons/19.png",
      "256": "data/icons/256.png",
      "32": "data/icons/32.png",
      "38": "data/icons/38.png",
      "48": "data/icons/48.png",
      "512": "data/icons/512.png",
      "64": "data/icons/64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Open in VLC™ media player",
   "optional_permissions": [ "downloads", "clipboardWrite" ],
   "options_ui": {
      "chrome_style": true,
      "page": "data/options/index.html"
   },
   "page_action": {
      "default_icon": {
         "16": "data/icons/16.png",
         "32": "data/icons/32.png"
      }
   },
   "permissions": [ "storage", "contextMenus", "nativeMessaging", "webRequest", "notifications", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.2",
   "web_accessible_resources": [ "/data/inject/*" ]
}
